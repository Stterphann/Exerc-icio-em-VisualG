Algoritmo "PROGRAMA1"



Var

 c, d, f : real



Inicio


  
  escreval (" Bem -vindo")
  escreval ("Digite a distância percorrida: ")

  leia (d)
  
  escreval ("Digite o combustível gasto: ")

  leia (c)
  
  f <- d / c
  
  escreval (" Consumo médio é de ", f:8:4)
  
  
Fimalgoritmo